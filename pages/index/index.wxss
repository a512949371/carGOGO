/**index.wxss**/
.container{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  -webkit-tap-highlight-color: rgba(0,0,0,0);
  -webkit-overflow-scrolling: touch;
}
.flex-box{
  display: flex;
  align-items: center;
}
.pt20{
 padding-top: 20rpx;
}
.userbtn{
  position: fixed;
  top: 80rpx;
  right: 40rpx;
  width: 60rpx;
  font-size: 24rpx;
}
.left-nav{
  position: fixed;
  top:0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9999
}
.left-nav .shaw{
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5)
  }
.user-box{
  position: fixed;
  top: 0;
  left: -149px;
  bottom: 0;
  width: 150px;
  background-color: #252525;
  box-sizing: border-box;
  color: #FFF
}
.user{
  padding: 20rpx 40rpx; 
  margin-top: 40rpx;
  font-size: 34rpx;
  font-weight: bold
}
.user-box .img{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 40rpx;
}
.nav{
  padding: 20rpx 40rpx;
  font-size: 32rpx;
}
.nav .img{
  margin-right: 40rpx;
  text-align: center;
  line-height: 70rpx;
}
.nav .img image{
  display: inline-block;
}
.badge{
  padding: 0 40rpx;
  color: #b2b2b2;
  font-size: 24rpx;
}
.badge .img{
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  margin: auto
}
.badge .img image{
  width: 100rpx;
  height: 100rpx;
  display: inline-block
}
.map-box{
  position: fixed;
  width: 40px;
  right: 20px;
  bottom: 50px;
  font-size: 24rpx;
}
.time-box{
  position: fixed;
  left:0;
  right: 0;
  bottom: 0;
  z-index: 999;
  background-color: #FFF;
  border-top: 1px solid #e5e5e5
  }
.time-box .shaw{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 1000;
}
.time-box .content{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 10001;
  background-color: #FFF
}
.time-box .show-time{
  text-align: center;
  padding:20rpx;
  font-size: 30rpx;
  justify-content: space-between
}
.time-box .show-time .btn{
  color: #3e3efe
}
.time-box picker-view{
  text-align: center
}
swiper{
  height: 100%;
}
.rentalcar-tip{
  position: fixed;
  top:0;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 99
}
.rentalcar-tip .shaw{
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 100;  
}
.rentalcar-tip .rentalcar{
  position: absolute;
  left: 40rpx;
  right: 40rpx;
  top: 40%;
  background-color: #FFF;
  border-radius: 10rpx;
  z-index: 101
}
.rentalcar-tip .rentalcar .tit{
  text-align: center;
  padding: 30rpx 40rpx;
}
.rentalcar-tip .rentalcar .text{
  text-align: left;
  font-size: 28rpx;
  color: #666;
  padding: 10rpx 40rpx;
}
.rentalcar-tip .rentalcar .btn-box{
  border-top: 1px solid #909090;
  margin-top: 20rpx;
  padding: 0 40rpx;
}
.rentalcar-tip .rentalcar .btn-box .btn{
  flex: 1;
  font-size: 36rpx;
  text-align: center;
  padding: 16rpx 0;
  color: #666
}
.border-2{
  border-left: 2px solid #909090
}
.list-box{
  width: 100%;
  height: 100%;
  position: relative;
  }
.list-1{
  background: url(http://p8x2zn192.bkt.clouddn.com/bg_01.jpg) no-repeat top left;
  background-size: cover;
}
.list-2{
  background: url(http://p8x2zn192.bkt.clouddn.com/bg_02.jpg) no-repeat top left;
  background-size: cover;
}
.list-3{
  background: url(http://p8x2zn192.bkt.clouddn.com/bg_05@2x.png) no-repeat top left;
  background-size: cover;
}
.list-4{
  background: url(http://p8x2zn192.bkt.clouddn.com/bg_03@2x.png) no-repeat top left;
  background-size: cover;
}
.list-5{
  background: url(http://p8x2zn192.bkt.clouddn.com/bg_04.png) no-repeat top left;
  background-size: cover;
}
.list-box .top-pic{
  width: 100%;
  position: absolute;
  top: 10rpx;
  left: 0;
  right: 0;
  z-index: 5;
  padding: 0 20rpx;
  box-sizing: border-box
}
.list-box .top-pic image{
  width: 100%;
  height: 420rpx;
}
image{
  width: 100%;
  display:inline-block;
}
.list-box .left-pic{
  position: absolute;
  top: 380rpx;
  left: 22rpx;
}
.list-box .left-pic image{
  width: 360rpx;
  height: 450rpx;
}
.list-box .right-pic1{
  position: absolute;
  top: 380rpx;
  right: 22rpx;
}
.list-box .right-pic1 image{
  width: 510rpx;
  height: 262rpx;

}
.list-box .right-pic2{
  position: absolute;
  top: 552rpx;
  right: 22rpx;
  
}
.list-box .right-pic2 image{
  width: 388rpx;
  height: 278rpx;
}
.list-box .direction{
  position: absolute;
  bottom: 40px;
  left: calc(50% - 50rpx);
  width: 100rpx;
}
.list-box .carinfo-box{
  position: absolute;
  bottom: 150rpx;
  left: 20rpx;
  right: 20rpx;
}
.list-box .carinfo-box .car-name{
  justify-content: space-between
}
.list-box .carinfo-box .car-name .name{
  font-size: 42rpx;
  color: #FFF;
  font-weight: bold;
  flex: 1
}
.list-box .carinfo-box .car-name .rule{
  width: 160rpx;
  color: #FFF;
  font-size: 30rpx;
  text-decoration: underline
}
.list-box .carinfo-box .desc{
  color: #FFF;
  font-size: 30rpx;  
}
.list-box .carinfo-box .desc .doc{
  width: 10rpx;
  height: 10rpx;
  background-color: #FFF;
  border-radius: 10rpx;
  display: inline-block;
  margin: 0 10rpx;
}
.list-box .carinfo-box .desc .icon-map{
  width: 20rpx;
  height: 27rpx;
  display: inline-block
}
.list-box .carinfo-box .desc .icon-map image{
  width: 20rpx;
  height: 27rpx;
  display: inline-block;
  margin-left: 20rpx;
}
.list-box .footer{
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 100rpx;
  z-index: 999;
}
.list-box .footer .foot-box{
  justify-content: space-between
}
.list-box .footer .foot-box .left{
  flex: 1;
  padding-left: 20rpx;
}
.list-box .footer .foot-box .left .warm{
  font-size: 36rpx;
  color: #ffde00
}
.list-box .footer .foot-box .left .ordinary{
  padding-top: 10rpx;
  font-size: 28rpx;
  color: #FFF
}
.list-box .footer .foot-box .right{
  width: 300rpx;
  margin-right: 20rpx;
}
.list-box .footer .foot-box .right image{
  width: 300rpx;
  height: 70rpx;
}
.z-index{
  z-index: 99
}